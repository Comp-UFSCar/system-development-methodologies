@model IEnumerable<Employee>

@Html.Partial("_PagedListPager", Model)

@foreach (var employee in Model)
{
    @Html.Partial("_Profile", employee)
    
    <br />
    <div class="btn-group col-sm-offset-1">
        @Html.ActionLink("Edit", "Edit", new { id = employee.Id }, new { @class = "btn btn-default" })
        @Html.ActionLink("Details", "Details", new { id = employee.Id }, new { @class = "btn btn-default" })
        @Html.ActionLink("Delete", "Delete", new { id = employee.Id }, new { @class = "btn btn-default" })
    </div>
    <br /><br /><br />
}
