@model IEnumerable<Game>

@Html.Partial("_PagedListPager", Model)

@foreach (var game in Model)
{
    @Html.Partial("_Item", game)
    
    <div class="btn-group col-sm-offset-4">
        @Html.ActionLink("Edit", "Edit", new { id = game.Id }, new { @class = "btn btn-default" })
        @Html.ActionLink("Delete", "Delete", new { id = game.Id }, new { @class = "btn btn-default" })
    </div>
    <br /><br /><br />
}
