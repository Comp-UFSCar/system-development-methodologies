@model string

<a href="@Url.Action("Edit", new { id = Model })"
   class="btn btn-default" title="Edit record #@Model"
   tabindex="0">
    <span class="glyphicon glyphicon-pencil"></span> Edit
</a>

<a href="@Url.Action("Delete", new { id = Model })"
   class="btn btn-primary" title="Delete record #@Model"
   tabindex="0">
    <span class=" glyphicon glyphicon-remove"></span> Delete
</a>
